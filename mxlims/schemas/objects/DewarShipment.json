{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "Shipment of Dewars, with typed JSON containment lists",
    "title": "DewarShipment",
    "type": "object",
    "id": "DewarShipment.json",
    "allOf": [
        {
            "$ref": "../rawobjects/RawShipment.json"
        }
    ],
    "properties": {
        "contents": {
            "type": "array",
            "items": {
                "$ref": "../objects/Dewar.json"
            },
            "minItems": 1
        }
    }
}
