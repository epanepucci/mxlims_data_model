{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "A puck containing mounted crystals on pins, with typed JSON containment lists.",
    "title": "Puck",
    "type": "object",
    "id": "Puck.json",
    "allOf": [
        {
            "$ref": "../rawobjects/RawPuck.json"
        }
    ],
    "properties": {
        "contents": {
            "type": "array",
            "items": {
                "$ref": "../objects/Pin.json"
            },
            "minItems": 1
        }
    }
}
